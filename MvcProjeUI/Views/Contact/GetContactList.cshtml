@using EntityLayer.Concrete
@model List<Contact>
@{
    ViewBag.Title = "GetContactList";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<h2>İletişim Listeleme</h2>

<table class="table table-hover">
    <tr>
        <td>İletişim Id</td>
        <td>Kullanıcı Adı</td>
        <td>Konu</td>
        <td>Mesaj</td>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @item.ContactId
    </td>
    <td>
        @item.UserName
    </td>
    <td>
        @item.UserMail
    </td>
    <td>
        @item.Subject
    </td>
    <td>
        @item.Message
    </td>
</tr>
    }
</table>
<a href="/Contact/AddContact" class="btn btn-info">Yeni İletişim</a>
<a href="/Home/Index" class="btn btn-info">Ana Sayfa</a>